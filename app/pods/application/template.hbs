<div class="object-browser layout-row">
    <div class="input-fields container flex-20">
        test
    </div>
    
    <div class="object-list layout-column flex-80">
        {{#visjs-network class="graph-view container"}}
                {{visjs-node nId="0" label="0"}}
                {{visjs-node nId="1" label="1"}}
                {{visjs-node nId="2" label="2"}}
                {{visjs-node nId="3" label="3"}}
                {{visjs-node nId="4" label="4"}}
                {{visjs-node nId="5" label="5"}}
                {{visjs-node nId="6" label="6"}}
                {{visjs-node nId="7" label="7"}}
                {{visjs-node nId="8" label="8"}}
                {{visjs-node nId="9" label="9"}}
            {{#each model as |x|}}
                {{visjs-edge from=x.source to=x.target}}
            {{/each}}
        {{/visjs-network}}
        {{#paper-list class="scroll-div container flex-50"}}
            {{#paper-subheader}}Mock Network Events{{/paper-subheader}}
            {{#each model as |item|}}
                {{#paper-item class="md-3-line"}}
                {{item.id}}
                <div class="md-list-item-text">
                    <p>Source: {{item.source}} Target {{item.target}}</p>
                </div>
                {{/paper-item}}
            {{/each}}
        {{/paper-list}}
        
    </div>
</div>
{{outlet}}